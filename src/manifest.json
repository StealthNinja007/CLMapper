{
    "name": "CLMapper",
    "version": "1.2.2.0",
    "manifest_version": 2,
    "description": "Craigslist posts shown on a map.",
    "content_scripts": [
        {
            "matches": [ "*://*.craigslist.org/*", "*://*.craigslist.com.mx/*", "*://*.craigslist.co.uk/*", "*://*.craigslist.ca/*", "*://*.craigslist.de/*", "*://*.craigslist.co.kr/*", "*://*.craigslist.co.in/*", "*://*.craigslist.se/*" ],
            "include_globs": [ "*/hhh*", "*/apa*", "*/roo*", "*/sub*", "*/hsw*", "*/swp*", "*/vac*", "*/prk*", "*/off*", "*/rea*", "*/aap*", "*/abo*", "*/nfb*", "*/fee*", "*/nfa*", "*/aiv*" ],
            "exclude_matches": [
                "*://www.craigslist.org/*",
                "*://craigslist.org/*"
            ],
            "js": [
                "libs/jquery.js",
                "libs/lscache.js",
                "libs/PageInterface.js",
                "libs/Geocoder.js",
                "pages/housing/index/content_script.js",
                "main.js"
            ],
            "run_at": "document_end"
        }
    ],
    "web_accessible_resources": [
        "pages/housing/index/bootstrap.css",
        "pages/housing/index/page_style.css",
        "pages/housing/index/page_script.js",
        "libs/bootstrap/bootstrap-collapse.js",
        "images/marker-normal.png",
        "images/marker-normal-visited.png",
        "images/marker-fee.png",
        "images/marker-fee-visited.png",
        "images/marker-blank.png",
        "images/marker-shadow.png",
        "images/donate-green.png"
    ],
    "icons": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    },
    "permissions": [ "http://maps.googleapis.com/", "*://*.craigslist.org/", "*://*.craigslist.com.mx/", "*://*.craigslist.co.uk/", "*://*.craigslist.ca/", "*://*.craigslist.de/", "*://*.craigslist.co.kr/", "*://*.craigslist.co.in/", "*://*.craigslist.se/" ]
}
